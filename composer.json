{
   "name":"webpatser/elastic-apm-laravel",
   "keywords":[
      "laravel",
      "elastic",
      "APM",
      "Lumen",
      "monitoring",
      "debug"
   ],
   "description":"A package to integrate Elastic APM into Laravel",
   "license":"MIT",
   "require":{
      "php":">= 7.2",
      "illuminate/database":"^6.0",
      "illuminate/http":"^6.0",
      "illuminate/routing":"^6.0",
      "illuminate/support":"^6.0",
      "philkra/elastic-apm-php-agent":"^7.0",
      "ramsey/uuid":"^3.0"
   },
   "require-dev":{
      "phpunit/phpunit":"6.*"
   },
   "autoload":{
      "psr-4":{
         "PhilKra\\ElasticApmLaravel\\":"src/"
      }
   },
   "autoload-dev":{
      "psr-4":{
         "PhilKra\\Tests\\":"tests/"
      }
   },
   "extra":{
      "laravel":{
         "providers":[
            "PhilKra\\ElasticApmLaravel\\Providers\\ElasticApmServiceProvider"
         ],
         "aliases":{
            "ElasticApm":"PhilKra\\ElasticApmLaravel\\Facades\\ElasticApm"
         }
      }
   },
   "config":{
      "optimize-autoloader":true
   },
   "authors":[
      {
         "name":"Philip Krauss",
         "email":"philip@philipkrauss.at",
         "homepage":"https://github.com/philkra/elastic-apm-laravel"
      },
      {
         "name":"George Boot",
         "email":"george@entryninja.com"
      }
   ]
}
